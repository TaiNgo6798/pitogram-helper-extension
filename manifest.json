{
  "name": "Pitogram Helper",
  "version": "1.0.0",
  "description": "Pitogram Helper",
  "manifest_version": 3,
  "author": "TaiNgo",
  "action": {
    "default_popup": "index.html",
    "default_title": "Pitogram Helper"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["cookies", "tabs", "activeTab", "scripting"],
  "host_permissions": ["*://*.instagram.com/", "*://*.pitogram.vercel.app/"]
}
